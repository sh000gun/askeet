<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0">
  <channel>
  <title>{% trans %}Popular questions on askeet{% endtrans %}</title>
  <link>{{ path('question_list') }}</link>
  <description>{% trans %}A list of the most popular questions asked on the askeet site, rated by the community{% endtrans %}.</description>
  <language>en</language>
{% for question in questions %}
<item>
  <title>{{ question.title }}</title>
  <description>{{ question.body }}</description>
  <link>{{url('question_show', {stripped_title: question.getStrippedTitle}) }}</link>
  <guid>{{ guid }}</guid>
  <pubDate>{{ time|date('D, d M Y G:i:s')|format_datetime() }}</pubDate>
</item>
{% endfor %}
  </channel>
</rss>
